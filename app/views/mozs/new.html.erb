<h1>Mozs#new</h1>
<p>Find me in app/views/mozs/new.html.erb</p>
<%= form_with model: @moz do |f| %>
    <h5>もずく</h5>
    <div class="post_form">
      <%= f.radio_button :mozuku, 5 ,id: 'star1'%>
      <label for="star1"><span class="text">最高</span>★</label>
  
      <%= f.radio_button :mozuku, 4 ,id: 'star2'%>
      <label for="star2"><span class="text">良い</span>★</label>
  
      <%= f.radio_button :mozuku, 3 ,id: 'star3'%>
      <label for="star3"><span class="text">普通</span>★</label>
  
      <%= f.radio_button :mozuku, 2 ,id: 'star4'%>
      <label for="star4"><span class="text">悪い</span>★</label>
  
      <%= f.radio_button :mozuku, 1 ,id: 'star5'%>
      <label for="star5"><span class="text">最悪</span>★</label>
    </div>
  
    <h5>のぞみん</h5>
    <div class="post_form">
      <%= f.radio_button :nozomi,  5 ,id: 'star6'%>
      <label for="star6"><span class="text">最高</span>★</label>
  
      <%= f.radio_button :nozomi,  4 ,id: 'star7'%>
      <label for="star7"><span class="text">良い</span>★</label>
  
      <%= f.radio_button :nozomi, 3 ,id: 'star8'%>
      <label for="star8"><span class="text">普通</span>★</label>
  
      <%= f.radio_button :nozomi , 2 ,id: 'star9'%>
      <label for="star9"><span class="text">悪い</span>★</label>
  
      <%= f.radio_button :nozomi, 1 ,id: 'star10'%>
      <label for="star10"><span class="text">最悪</span>★</label>
    </div>
    <%= f.submit '送信'%>
  <% end %>
  